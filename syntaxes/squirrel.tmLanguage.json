	"repository": {
		"code-blocks": {
			"patterns": [
				{
					"include": "#control-keywords"
				},
				{
					"include": "#other-keywords"
				},
				{
					"include": "#modifiers"
				},
				{
					"include": "#literals"
				},
				{
					"include": "#class-declarations"
				},
				{
