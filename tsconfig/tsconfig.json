{
  "compilerOptions": {
    "target": "ES2024",
    "lib": ["ESNext"],
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
	"noFallthroughCasesInSwitch": true,
	"resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
	"esModuleInterop": true
  }
}
