{
	"fori": {
		"prefix": "fori",
		"body": [
			"for (local ${1:i} = 0; ${1:i} < $2; ${1:i}++) {",
			"\t$0",
			"}"
		]
	},
	"foreach": {
		"prefix": "foreach",
		"body": [
			"foreach (${1:item} in ${2:table}) {",
			"\t$0",
			"}"
		]
	},
	"foreachi": {
		"prefix": "foreachi",
		"body": [
			"foreach (${1:i}, ${2:item} in ${3:table}) {",
			"\t$0",
			"}"
		]
	},
	"(name params) {...}": {
		"prefix": "function",
		"body": [
			"function ${1:f}(${2}) {",
			"\t$0",
			"}"
		]
	}
}